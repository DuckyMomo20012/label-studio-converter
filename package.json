{
  "name": "label-studio-converter",
  "version": "1.3.1",
  "author": "DuckyMomo20012",
  "description": "Convert between Label Studio OCR format and PPOCRLabelv2 format",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuckyMomo20012/label-studio-converter.git"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "liturgical"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "label-studio-converter": "./dist/cli.js",
    "__label-studio-converter_bash_complete": "./dist/bash-complete.js"
  },
  "scripts": {
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/ test/",
    "lint:fix": "eslint src/ test/ --fix",
    "build": "tsc && tsup",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "check-all": "pnpm run type-check && pnpm run lint && pnpm run test:run",
    "prepublish": "pnpm run build && pnpm run check-exports && pnpm run check-all",
    "postinstall": "npx @stricli/auto-complete@latest install label-studio-converter --bash __label-studio-converter_bash_complete"
  },
  "dependencies": {
    "@stricli/auto-complete": "1.2.5",
    "@stricli/core": "1.2.5",
    "@turf/turf": "7.3.2",
    "chalk": "5.6.2",
    "es-toolkit": "1.44.0",
    "image-size": "2.0.2",
    "type-fest": "5.4.1",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@babel/core": "7.28.6",
    "@commitlint/cli": "20.3.1",
    "@commitlint/config-conventional": "20.3.1",
    "@types/node": "25.0.9",
    "@typescript-eslint/eslint-plugin": "8.53.1",
    "@typescript-eslint/parser": "8.53.1",
    "@vitest/eslint-plugin": "1.6.6",
    "@vitest/ui": "4.0.17",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.5",
    "globals": "17.0.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.0",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.17"
  }
}
